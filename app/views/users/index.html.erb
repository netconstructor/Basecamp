<center>
	<div class="Login">
		<%
			@user=User.new
			form_for :user, @user, :url=>{:action => "login"} do |f|
		%>
		<%= f.label "Username"%><br>
		<%= f.text_field :username%><br>
		<%= f.label "Password"%><br>
		<%= f.password_field :password%><br><br>
		<%= submit_tag 'Login' %>
		
		<%end%>
	</div>
</center>
